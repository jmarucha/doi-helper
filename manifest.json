{

    "manifest_version": 2,
    "name": "DOI Helper",
    "version": "1.0",
  
    "description": "Adds a red border to all webpages matching mozilla.org.",
  
    "icons": {
    },
  
    "content_scripts": [
      {
        "matches": ["*://inspirehep.net/*"],
        "js": ["inspire-hep.js"]
      },
      {
        "matches": ["*://*.sciencedirect.com/*"],
        "js": ["science-direct.js"]
      }
    ]
  
  }